<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.uwm.wundergrads.diabetesselfmanagement_wundergrads">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GraphSelection" />
        <activity
            android:name=".GraphDisplay"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_graph_display"
            android:theme="@style/FullscreenTheme" />
        <activity
            android:name=".TableDisplay"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_table_display"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".BGLInput" />
        <activity android:name=".DietInput" />
        <activity android:name=".ExerciseInput" />
        <activity android:name=".MedicationInput" />
        <activity android:name=".RegimenInput"></activity>
    </application>

</manifest>